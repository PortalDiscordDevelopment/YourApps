{
	"search.exclude": {
		"**/.yarn": true,
		"**/.pnp.*": true,
		"**/dist": true
	},
	"eslint.nodePath": ".yarn/sdks",
	"typescript.tsdk": ".yarn/sdks/typescript/lib",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"i18n-ally.localesPaths": ["src/languages"],
	"i18n-ally.pathMatcher": "{locale}/{namespaces}.{ext}",
	"i18n-ally.keystyle": "nested",
	"i18n-ally.extract.keygenStyle": "ALL_CAPS",
	"i18n-ally.refactor.templates": [
		{
			"template": "this.client.t('{key}')"
		}
	],
	"i18n-ally.defaultNamespace": "bot",
	"i18n-ally.namespace": true,
	"i18n-ally.extract.autoDetect": true,
	"i18n-ally.annotationMaxLength": 70,
	"prettier.prettierPath": ".yarn/sdks/prettier/index.js",
	"i18n-ally.extract.ignoredByFiles": {
		"src/commands/applications/apply.ts": ["âœ–"]
	},
	"yaml.schemas": {
		"https://raw.githubusercontent.com/TymanWasTaken/confgen/master/.confgen.schema.json": [
			".confgen.yaml",
			"file:///workspace/YourApps_JS/.confgen.yaml"
		]
	},
	"i18n-ally.enabledFrameworks": ["custom"],
	"editor.acceptSuggestionOnCommitCharacter": false,
	"i18n-ally.keysInUse": [
		"bot.LOGGING.PREFIX_ADDED",
		"bot.LOGGING.PREFIX_REMOVED",
		"bot.LOGGING.ADMIN_ROLE_ADD",
		"bot.LOGGING.ADMIN_ROLE_REMOVED",
		"bot.LOGGING.BLACKLIST_ROLE_ADD",
		"bot.LOGGING.BLACKLIST_ROLE_REMOVED",
		"bot.LOGGING.REVIEW_ROLE_ADD",
		"bot.LOGGING.REVIEW_ROLE_REMOVED",
		"bot.LOGGING.LOGPING_ROLE_ADD",
		"bot.LOGGING.LOGPING_ROLE_REMOVED",
		"bot.LOGGING.CLOSE",
		"bot.LOGGING.OPEN",
		"bot.LOGGING.LOG_CHANNEL",
		"bot.LOGGING.ARCHIVE_CHANNEL",
		"bot.LOGGING.SUBMISSION_APPROVED",
		"bot.LOGGING.SUBMISSION_APPROVED_REASON",
		"bot.LOGGING.SUBMISSION_DENIED",
		"bot.LOGGING.APPLICATION_SUBMITTED"
	]
}
